<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zjh.stock.dao.StockDailyDataDao">

    <select id="getKline" resultType="com.zjh.stock.entity.StockDailyData">
        select *
        from stock_daily_data
        where code = #{code}
        order by date desc
        limit #{limit}
    </select>

</mapper>
